-- ===========================
-- TABLE: StorageDao
-- ===========================

CREATE TABLE StorageDao (
  id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
  title TEXT NOT NULL
);

insertStorageDao:
INSERT INTO StorageDao(title)
VALUES (?)
RETURNING id;

deleteStorageDao:
DELETE FROM StorageDao
WHERE id = ?
RETURNING id;

selectStorageDaoById:
SELECT * FROM StorageDao
WHERE id = ?;

selectAllStorageDao:
SELECT * FROM StorageDao;
